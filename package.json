{
  "name": "openai-image-api",
  "version": "2.0.0",
  "description": "OpenAI Image & Video Generation API wrapper - DALL-E, GPT Image, and Sora models for Node.js with CLI",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "type": "module",
  "bin": {
    "openai-img": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "import": "./dist/api.js"
    },
    "./video-api": {
      "types": "./dist/video-api.d.ts",
      "import": "./dist/video-api.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "openai": "node dist/cli.js",
    "openai:help": "node dist/cli.js --help",
    "openai:examples": "node dist/cli.js --examples",
    "openai:dalle2": "node dist/cli.js --dalle-2",
    "openai:dalle3": "node dist/cli.js --dalle-3",
    "openai:gpt-image": "node dist/cli.js --gpt-image-1",
    "openai:sora-2": "node dist/cli.js --video --sora-2",
    "openai:sora-2-pro": "node dist/cli.js --video --sora-2-pro",
    "openai:list-videos": "node dist/cli.js --list-videos",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "openai",
    "dall-e",
    "dall-e-2",
    "dall-e-3",
    "gpt-image-1",
    "sora",
    "sora-2",
    "sora-2-pro",
    "image-generation",
    "video-generation",
    "ai-image",
    "ai-video",
    "text-to-image",
    "text-to-video",
    "image-to-video",
    "image-edit",
    "image-variation",
    "video-remix",
    "cli",
    "typescript"
  ],
  "author": "img-gen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aself101/openai-image-api.git"
  },
  "bugs": {
    "url": "https://github.com/aself101/openai-image-api/issues"
  },
  "homepage": "https://github.com/aself101/openai-image-api#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.10",
    "semantic-release": "^24.2.9",
    "typescript": "^5.9.3",
    "vitest": "^4.0.10"
  }
}
